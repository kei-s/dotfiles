[user]
  email = kei.shiratsuchi@gmail.com
  name = kei-s
[color]
  ui = auto
[core]
  pager = lv -c
  excludesfile = ~/dotfiles/global.gitignore
[merge]
  tool = vimdiff
[alias]
  br = branch
  ci = commit -v
  co = checkout
  pr = pull --rebase
  ds = diff --staged
  dw = diff --word-diff
  lg = log --graph
  lp = log -p --stat
  lo = log --pretty=oneline
  st = status -sb
  cp = cherry-pick
  fo = fetch origin
[push]
  default = tracking
