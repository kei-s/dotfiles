[user]
  email = kei-s@mbc.nifty.com
[color]
  ui = auto
[core]
  pager = lv -c
  excludesfile = ~/dotfiles/global.gitignore
[merge]
  tool = vimdiff
[alias]
  br = branch
  ci = commit -v
  co = checkout
  pr = pull --rebase
  ds = diff --staged
  lg = log --graph
  lp = log -p --stat
  lo = log --pretty=oneline
  st = status
